	funcao defrasdf()
	{
		limpa()
		Util.aguarde(200)
		escreva("Está ")
		Util.aguarde(200)
		escreva("escrito: ")
		Util.aguarde(1000)
		escreva("\n\nDECIFRE:\n\n")
		Util.aguarde(800)
		escreva("QUeremos Aqueles TRês Odres.")
		Util.aguarde(800)
		escreva("\nsemPre façO tRabalho, TÁrdio e o voSso tempo??\n\nDecifre o código: ")
		Util.aguarde(800)
		
			// ordem : @ % # $
		
		cadeia decifre
		
		leia(decifre)

		se(decifre == "QUATROPORTAS")
		{

			escreva("A porta abriu...")
			Util.aguarde(1000)
			EXITFINAL()
			
		}
		senao se(decifre == "QUATROPORTÁS")
		{

			escreva("A porta abriu...")
			Util.aguarde(1000)
			EXITFINAL()
		}
		senao
		{
			escreva("Você ")
			Util.aguarde(200)
			escreva("Errou ")
			Util.aguarde(200)
			escreva("a ")
			Util.aguarde(200)
			escreva("senha ")
			Util.aguarde(1000)
			limpa()
			escreva("Tente ")
			Util.aguarde(200)
			escreva("novamente ")
			Util.aguarde(1000)
			
			jaera()
			
		}
		
		
	}
	funcao jaera()
	{	
		limpa()
		Util.aguarde(200)
		escreva("Está ")
		Util.aguarde(200)
		escreva("escrito: ")
		Util.aguarde(1000)
		escreva("\n\nDECIFRE:\n\n")
		Util.aguarde(800)
		escreva("QUeremos Aqueles TRês Odres.")
		Util.aguarde(800)
		escreva("\nsemPre façO tRabalho, TÁrdio e o voSso tempo??\n\nDecifre o código: ")
		Util.aguarde(800)
		
			// ordem : @ % # $
		
		cadeia decifre
		
		leia(decifre)

		se(decifre == "QUATROPORTAS")
		{

			escreva("A porta abriu...")
			Util.aguarde(1000)
			EXITFINAL()
			
		}
		senao se(decifre == "QUATROPORTÁS")
		{

			escreva("A porta abriu...")
			Util.aguarde(1000)
			EXITFINAL()
		}
		senao
		{
			escreva("Você ")
			Util.aguarde(200)
			escreva("Errou ")
