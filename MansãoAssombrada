			          Util.aguarde(3000)
			          limpa()
			          escreva("No ")
			          Util.aguarde(200)
			          escreva("bolso ")
			          Util.aguarde(200)
			          escreva("do ")
			          Util.aguarde(200)
			          escreva("casaco ")
			          Util.aguarde(200)
			          escreva("tem ")
			          Util.aguarde(200)
			          escreva("um ")
			          Util.aguarde(200)
			          escreva("cachecol ")
			          Util.aguarde(200)
			          escreva("de ")
			          Util.aguarde(200)
			          escreva("croche ")
			          Util.aguarde(200)
			          escreva("bem ")
			          Util.aguarde(30)
			          escreva("quente!\n\n")
			          Util.aguarde(2000)

			          escreva("Nesse lugar você não tem mais risco de ter uma confusão mental(Ela é causada pelo frio).")
					Util.aguarde(4000)
					
					sim()
					pare
         
		          }
		          senao
				{
			          escreva("dia de azar...")
			          Util.aguarde(3000)
			          limpa()
			          escreva("Você ")
			          Util.aguarde(200)
			          escreva("vê ")
			          Util.aguarde(200)
			          escreva("que ")
			          Util.aguarde(200)
			          escreva("o ")
			          Util.aguarde(200)
