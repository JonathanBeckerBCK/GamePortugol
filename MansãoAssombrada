			escreva(".")
			Util.aguarde(200)
			escreva(".")
			Util.aguarde(3000)
			limpa()

			Morruy()
			
		}

	}
	funcao EXITFINAL()
	{

		limpa()
     	escreva("CONQUISTA: V O C Ê   F U G I U !!\n")

     	   escreva("       ........................               \n")
             escreva("       .:####################+.               \n")
             escreva("       .:#%%%%%%%%%%%%%%%%%%#+.               \n")
             escreva("       .:##%###############%#+                \n")
             escreva("       .:##%###############%#+                \n")
             escreva("       .:##%#*+++++++++++##%#+                \n")
             escreva("       .:##%#*#:-=*==+=#*##%#+.               \n")
             escreva("       .:##%#*#-=EXIT+=#*##%#+                \n")
             escreva("        :##%#***********+##%#+                \n")
             escreva("        :##%###############%#+.               \n")
             escreva("        :##%###############%#+.               \n")
             escreva("        :##%###############%#+.               \n")
             escreva("        :#%%##############=##+.               \n")
             escreva("        :#%%##############+%#+.               \n")
             escreva("        :#%%##############%%#+.               \n")
             escreva("       .:#%%##############%%#+.               \n")
             escreva("       .:#%%##############%%#+.               \n")
             escreva("       .:#%%##############%%#*.               \n")
             escreva("       .:#%%##############%%#*.               \n")
             escreva("       .:#%%##############%%#*.               \n")
             escreva("       .:#%%##############%%#*..              \n")
             escreva("   .....-#%%##############%%#*....... ...     \n")
             escreva("........-#%%%#############%%#*...........     \n")
             escreva(".........=##################%#*:.............  \n")
   		   escreva(".............::::::::::::::::::................  \n")
  		   escreva("...............................................  \n")
  		   escreva("............................................. \n")
	}
	funcao Morruy()		
	{
		
		limpa()
     	escreva("CONQUISTA: M O R R E U    P O R   ? ? \n")

     		
		     	escreva("CONQUISTA: F I N A L    R U I M ")
		     	escreva("\n\n")
				escreva("                              *@@@#                        \n")                         
                    escreva("                         :@@@@@@@@@@@@@@@:                 \n")                          
                    escreva("    tenha              @@@@@@@@@@@@@@@@@@@@@               \n")                          
                    escreva("      mais           =@@@@@@@@@@@@@@@@@@@@@@@+             \n")                          
                    escreva("         atenção     @@@@@@@@@@@@@@@@@@@@@@@@@             \n")                          
                    escreva("                    +@@@@@@@@@@@@@@@@@@@@@@@@@+            \n")                          
                    escreva("     na              @@@@@@@@@@@@@@@@@@@@@@@@@.            \n")                          
                    escreva("       próxima       @@@@@     @@@@@     @@@@@             \n")                          
                    escreva("             vez!    @@@       @@@       #@@              \n")                          
                    escreva("                       @@     @@@@@@@     @@               \n")                          
                    escreva("                      =@@@@@@@@@   @@@@@@@@@=              \n")                          
                    escreva("                          -@@@@@@@@@@@@@-                  \n")                          
                    escreva("               @@@@         @@@@@@@@@@@         @@@@         \n")                          
                    escreva("              =@@@@@        =@@@@@@@@@=        @@@@@=        \n")                          
                    escreva("                :@@@@@@@@@=             -@@@@@@@@@-      \n")                          
                    escreva("                 @@@@@@@@@@@@@@     @@@@@@@@@@@@@@       \n")                          
                    escreva("                             @@@@@@@@@                   \n")                          
                    escreva("                          @@@@@@@@@@@@@@@                \n")                          
                    escreva("                @@@@@@@@@@@@*         +@@@@@@@@@@@@      \n")                          
                    escreva("                 =@@@@@@@                 @@@@@@@*       \n")                          
                    escreva("                  %@@@@                     @@@@%        \n")                          
                    escreva("                    @=                       -@      	  \n")
	
	}
     funcao lugart()
     {
     	     
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@%%##***++::+**********+++++++++++*+: .-========++++++*####**++++++\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@@%%#****++-.+**********++++++==+=-  :-----====++++*#*=++++++++**##\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@@%#*+++++++======-------::::...   .:::::::-------------------=====\n")     
     Util.aguarde(30)
     escreva("++*%@@@@@@@@@@@@@@@@@@@@@@@@*+=::=++++++++++++++=====--===+++.+++++++++++===.++**########*\n")     
     Util.aguarde(30)
     escreva("***#%@@@@@@@@@@@@@@@@@@@@.:*@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@\n")     
     Util.aguarde(30)
     escreva("#**#@@@@@@@@@@@@@@@@@@@@@::-+@@#=#@@%@@%#**++++*#%@@%#%@#+*@@@@@@@@@@@@@@@@@@@%*%@@@@@#@@@\n")
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@%%@@@@@@@@@.:-=@@%@@%@%%@@@%#######+#+#%%@@%*@@@@@@@*@@@@@@@@@@@==*@@@@@*@@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@%%%@.:-=@@=-*=#@@@: : ::-:.+ :@@#*#@@:@@@@@@:-+@@@@@@%%#@@@%@@@@@.   \n")     
     Util.aguarde(30)
     escreva("         ::+#@@@@@@@@%#*%--:=@@++#-*@@@+-=. .-. -.+@@#+*#- #@@@@@==%@@@@@@@@%#@+%@@@@@:=-:\n")     
     Util.aguarde(30)
     escreva("           .-@@@@@@@@@%@@@=:-@@=#*-+@@@#**+ :*+=*+@*@@%#   *@@@@@@*-+@%@@@@@@*+@%@@@@@=#*#\n")     
     Util.aguarde(30)
     escreva("           .:@@@@@@@@@@@@@=:-@@=@@@*@@@@%@@@@@*+@@@@@@%#@@-@@@@@@@@%*%@#@@@@@@=+@@@@@@*@@@\n")     
     Util.aguarde(30)
     escreva("    .      :=@@@@@@@@@@@@@=:-@@*@@@@@@@@@@@@@##=@%+@@%@@@@*@@@@@@@@@@@%%@@@@@@@@@@@@@@#@@@\n")     
     Util.aguarde(30)
     escreva(".   .:-:*%@@@@@@@@@@@@@@@@@@@@@#@@@#=+-+-+-+-;+-=-=-+_+_-;+-=-=-+_+_-;+-=-=-+_+_-;+-=-=-+_\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@@@@@@#=+*%###%@%%#%@@#*#@*%@@@@@@@@@#@@@@@%**#+==---=+**++=@@@@@@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@@@%#@+*@@@@@@@++.-@@@@@@#**##+%@@#*##@@@@@@@@@@@#%@@@%*=:::+@@@@@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@%=:::---@@@@@==-:-@@@@+:--=+**#@@@@@@@@@@@@@@@%%@@@=-\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@%*+#@@@@@@@@@%@@@@@@@@@@@@@@%===*@@@@#===+**#%@@@@@@@@@@@.--+@**@@\n")    
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@%#*++--@@@@@@@@#+@@@@@@@@@@@@@@@-:--=%@@@@@%*+**##%@@@@@@@*-.-+**##@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@%#*++=--+@@@@@@@@--+@@@@@@@==-:=-=-..--:-=@@@@@@@#**#%%%%%%%@%#*++-:=#@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@##*++==--=**+++++++===--------.......   .+===---------=====#@#%+%%%%@%##%@*\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@#*++==-:--=+++=+==++=-===-------=........    -=-----:::::---=*@#@@@@@@@@@@@@@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@#**+=----::--++++++=+==----*========+........     -------:::::::=%#@@@@@@@@@@@@@@@\n")     
     Util.aguarde(30)
     escreva("@@@@@#*#**+==----==**+++++==------+*++++++++*-:::..:.:..  .=+===-=------===%@@@@@@@@@@@@@@\n")
	Util.aguarde(30)
	escreva("                             || Lugar ")
	Util.aguarde(30)
	escreva("desconhecido ||\n")
	Util.aguarde(2000)
     escreva("                             ||   Ameaça:4 de 10   ||")
     Util.aguarde(800)
     escreva("\nAMEAÇAS: comidas venenosas, confusão mental, escorpiôes e 'presença estranha'")
     escreva("(como se algo te dissese para sair o mais rapido possivel).\n\n")

	inteiro sair
		
		escreva("\nAVISO, LEIA AS INFORMAÇÔES, ESCREVA 1 PARA SAIR:")
		leia(sair)
		se(sair == 1)
		{
			DA()
		}
     
     }
	funcao imagemDado()
	{
		
				
		escreva("\n\n	      -**_**-*-*--**-*%%%%*-                     \n")
          escreva("           +%##. .=+*#-   -#=  =##%%%=              \n")
          escreva("       =%%%%%%##*:.:*#.  :     -**+:  =%.           \n")
          escreva("       #%@%+ *%%%%%%####:  +##=:.+*#*=*%%#          \n")
          escreva("       #%%@@@@%%%%%%%%#*- +*#####+-%%%%#*+          \n")
          escreva("      *###%%%%@@@@@%%@@%%#-..:%@%%%##***+           \n")
          escreva("      *##%##%##%%@@@@@@@@@@@@%%####*****+           \n")
          escreva("      -%%#.    .#%%%%@@@@@%%#####*    +*+           \n")
          escreva("      .%%=   #%  :#%%%%@@@@%%##+  +-  +++           \n")
          escreva("       #%-  -##.  ##%%%%@%%%%#-  =*..+**=           \n")
          escreva("       #%+      =##%%%%%%%%%##   =+****+            \n")
          escreva("       #%#   #%   #%%%%%%%%###    +  +*+            \n")
          escreva("       :#%#  ##*   %%%%%%%%###*  *+ .**-            \n")
          escreva("        :##%= +-   #%%%%%%%###   . =**.             \n")
          escreva("           #%%#: .+%%%%%%%%###. .+##=               \n")
          escreva("              *%@%%%%%%%%%%%#####*                  \n")
          escreva("                 =@@%%%%%%%%##*                     \n")
          escreva("                     @%%%%%%                        \n")

          



      }


      
}
