    		escreva("pode ")
     	Util.aguarde(200)	
		
    		escreva("ser ")
     	Util.aguarde(200)
     	
    		escreva("importante. ")
     	Util.aguarde(2000)	

		limpa()

		escreva("\n============================----=================----===========---------=======\n")
          escreva("=====-.========.-=======:.========.-=======-..=======.-=======-.========.-======\n")
 		escreva("=====:__________-=======:_________--=======+ _________-=======* __________-=====\n")
 		escreva("====+|  ....... |=======|  ......:|-=======+| ........|=======*| .........|=====\n")
 		escreva("====+|    @  .. |=======|  . %  .:|-=======+| .  # ...|=======*|     $ ...|=====\n")
 		escreva("====+|       .. |======+|       .:|-=======+|       ..|=======*|       ...|=====\n")
 		escreva("====+| *      . |======+| :      :|-=======+|-.    ...|=======*|+      ...|=====\n")
 		escreva("====+| ........ |======+| ...... :|-=======+|.........|=======*|...... ...|=====\n")
 		escreva("====+|  ....... |======+|  ......:|-=======+| ........|=======*| .........|=====\n")
		escreva("====+|        . |=======|        :|-=======+| .    ...|=======*|       ...|=====\n")
		escreva("-----|.       ..|-------|:  ..   .| --------|  . .  ..|:-:-----|  .     ..|:::::\n")
		escreva("________________________________________________________________________________\n")
		escreva("________________________________________________________________________________\n")
		escreva("________________________________________________________________________________\n")
		escreva("________________________________________________________________________________\n")
		escreva("________________________________________________________________________________\n")
		escreva("________________________________________________________________________________\n")
		
		escreva("\nVocê ")
     	Util.aguarde(600)
     	
		escreva("nota ")
     	Util.aguarde(200)
     	
     	escreva("que ")
     	Util.aguarde(200)
     	
     	escreva("apenas ")
     	Util.aguarde(200)
     	
    		escreva("a ")
     	Util.aguarde(200)	
		
    		escreva("primeira ")
     	Util.aguarde(200)
     	
    		escreva("porta ")
     	Util.aguarde(200)	
    		
    		escreva("esta ")
     	Util.aguarde(200)

    		escreva("aberta.")
     	Util.aguarde(2500)


	}
	funcao decisaoAoDesafio()
	{
		
		inteiro vPOR
		
		escreva("Você")
		Util.aguarde(200)
		
		escreva(" quer ")
		Util.aguarde(200)
		
		escreva("entrar? ")
		Util.aguarde(200)
	
		escreva("\n\nSim[1]  Não[2]\n\nSua escolha:")
		
    		leia(vPOR)

		    		
			escolha(vPOR)
			{
			caso 1:

				limpa()
				mansaoEstaDiferente()
				pare
				
		    	caso 2:

					limpa()
			
     				
    					escreva("você ")
     				Util.aguarde(200)

    					escreva("tenta ")
     				Util.aguarde(200)
     				
    					escreva("abrir ")
     				Util.aguarde(200)
     				
    					escreva("a ")
     				Util.aguarde(200)
     				
    					escreva("porta ")
     				Util.aguarde(200)
     				
    					escreva("de ")
     				Util.aguarde(200)
     				
    					escreva("onde ")
     				Util.aguarde(200)
     				
    					escreva("você ")
     				Util.aguarde(200)
     				
    					escreva("veio, ")
     				Util.aguarde(1000)
     				
    					escreva("ela ")
     				Util.aguarde(200)
     				
    					escreva("está ")
     				Util.aguarde(200)
     				
    					escreva("trancada, ")
     				Util.aguarde(1000)
     				
    					escreva("você ")
     				Util.aguarde(200)
     				
    					escreva("está ")
     				Util.aguarde(200)
     				
    					escreva("preso.")
     				Util.aguarde(2000)

					limpa()
					
					semVolta()

					pare
     				
		    	}	


			
	}
	funcao semVolta()
	{
		
		inteiro vPOR=1
		
		escreva("Entrar?? ")
		Util.aguarde(200)

		
		escreva("\n\nSim[1]\n\nSua escolha:")
		
    		leia(vPOR)

    		se(vPOR == 1)
    		{

			mansaoEstaDiferente()
    			
    		}
		
	}
	funcao mansaoEstaDiferente()
	{

		limpa()
			escreva("Você ")
			Util.aguarde(200)
			escreva("entra ")
			Util.aguarde(200)
			escreva("na ")
			Util.aguarde(200)
			escreva("porta")
			Util.aguarde(200)
			
			Util.aguarde(800)
			escreva(".")
			Util.aguarde(800)
			escreva(".")
			Util.aguarde(800)
			escreva(".")
		
			escreva(" não ")
			Util.aguarde(200)
			escreva("parece ")
			Util.aguarde(200)
			escreva("que ")
			Util.aguarde(200)
			escreva("você ")
			Util.aguarde(200)
			escreva("está ")
			Util.aguarde(200)
			escreva("na ")
			Util.aguarde(200)
			escreva("mansão")
			Util.aguarde(2000)

		
     }	
}
