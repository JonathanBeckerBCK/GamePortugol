     	Util.aguarde(200)
     	
     	escreva("vida ")
     	Util.aguarde(200)	
		
    		escreva("normal !")
     	Util.aguarde(200)

escreva("\n	    	      ##              \n")
escreva("	         @@# @@@@.*@@.        \n")
escreva("	    +*-.:@@@@@@@@@@@@- -**    \n")
escreva("	    .@@@@@@@*.  .+@@@@@@@:    \n")
escreva("	    :%@@@: -@@@@@@- .@@@%:.   \n")
escreva("	 .@@@@@* #@@@@@@@@@@# +@@@@@. \n")
escreva("	   %@@% @@@@@@@@@@@@@@ #@@%   \n")
escreva("	 *@@@@:-@@@@@@@@@@@@@@=.@@@@# \n")
escreva("	 +@@@@:-@@@@@@@@@@@@@@-.@@@@* \n")
escreva("	   @@@% %@@@@@@@@@@@@@ #@@@   \n")
escreva("	 .@@@@@* *@@@@@@@@@@# +@@@@@. \n")
escreva("	     %@@@- :@@@@@@: :@@@%.    \n")
escreva("	    :@@@@@@@#:  :#@@@@@@@:    \n")
escreva("	    ++. @@@@@@@@@@@@@@ .+*    \n")
escreva("	       =@@@@@@@@@@@@@@*       \n")
escreva("	       @@@@@@@@@@@@@@@@.      \n")
escreva("	      %@@@@@@@%#@@@@@@@@      \n")
escreva("	     -@@@@@@@@:.@@@@@@@@=     \n")
escreva("	     @@@@%@@@+  +@@@@@@@@.    \n")
escreva("	    #@=  :@@#    #@@:  =@#    \n")
escreva("	   ..     *@:    :@*      .   \n")
escreva("         .-      -:      .-       ,  \n")



     }
     funcao dentromansao()
	{
		
