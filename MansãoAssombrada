			escreva("%@@@####*+++=-=@#%*%#***@#+--:-##*###%%@##@--=+++*##*#@@%\n")
			escreva("%%%@%###*++++=-###*%%*+*#**#-:-######%%@##@-=+++**#=#%@%%\n")
			escreva("@@%**@#+***++==%##*%%#**+=+--:=#%%###%%@##@+==+*##*#%%%@@\n")
			escreva("@@@@%#+%#+++++=%##*%###**==--:=%####%%@%##@#+****%%*#@@@@\n")
			escreva("@@@@@@%#*#@%#%%@##*%%%#*++===-=%##%%%%@%##@%###%@+@%@@@@@\n")
			escreva("@@@@@%@@@%#*#*#@%%*%%##**=----=%%%%%@@@%##@%#***%@@@@%%%@\n")
			escreva("@@@%%%%%%%%%%@%@%%*#*==-------=%%@%%%@%@##%%@%%%@@@@@@@@@\n")

			caso 2:

			limpa()
