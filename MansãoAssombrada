		     	Util.aguarde(200)
		     	escreva(".")
		     	Util.aguarde(200)
		     	escreva(".")
		     	Util.aguarde(200)
		     	escreva(".")
		     	Util.aguarde(3000)

				limpa()
				
				inteiro testesorteDADO=Util.sorteia(1, 10)

				escreva("Numero sorteado:",testesorteDADO,"\n\n")
				
				se(testesorteDADO > 5)
		          {
		               escreva("dia de sorte!")
		               Util.aguarde(3000)
		               limpa()
			          escreva("Você ")
			          Util.aguarde(200)
			          escreva("pega ")
			          Util.aguarde(200)
