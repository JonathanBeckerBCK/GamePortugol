	funcao decisaoAoDesafio()
	{
		
		inteiro vPOR
		
		escreva("Você")
		Util.aguarde(200)
		
		escreva(" quer ")
		Util.aguarde(200)
		
		escreva("entrar? ")
		Util.aguarde(200)
	
		escreva("\n\nSim[1]  Não[2]\n\nSua escolha:")
		
    		leia(vPOR)

		    		
			escolha(vPOR)
			{
			caso 1:

				limpa()
				mansaoEstaDiferente()
				pare
				
		    	caso 2:

					limpa()
			
     				
    					escreva("você ")
     				Util.aguarde(200)

    					escreva("tenta ")
     				Util.aguarde(200)
     				
    					escreva("abrir ")
     				Util.aguarde(200)
     				
    					escreva("a ")
     				Util.aguarde(200)
     				
    					escreva("porta ")
     				Util.aguarde(200)
     				
    					escreva("de ")
     				Util.aguarde(200)
     				
    					escreva("onde ")
     				Util.aguarde(200)
     				
    					escreva("você ")
     				Util.aguarde(200)
     				
    					escreva("veio, ")
     				Util.aguarde(1000)
     				
    					escreva("ela ")
     				Util.aguarde(200)
     				
    					escreva("está ")
     				Util.aguarde(200)
     				
    					escreva("trancada, ")
     				Util.aguarde(1000)
     				
    					escreva("você ")
     				Util.aguarde(200)
     				
    					escreva("está ")
     				Util.aguarde(200)
     				
    					escreva("preso.")
     				Util.aguarde(2000)

					limpa()
					
					semVolta()

					pare
     				
		    	}	


			
	}
	funcao semVolta()
	{
		
		inteiro vPOR=1
		
		escreva("Entrar?? ")
