			          Util.aguarde(200)
			          escreva("nas ")
			          Util.aguarde(200)
			          escreva("costas ")
			          Util.aguarde(200)
			          escreva("está ")
			          Util.aguarde(200)
			          escreva("todo ")
			          Util.aguarde(200)
			          escreva("rasgado")
			          Util.aguarde(200)
			          escreva("... ")
			          Util.aguarde(3000)
			         
			          escreva("tem ")
			          Util.aguarde(200)
			          escreva("nenhum ")
			          Util.aguarde(200)
			          escreva("casaco ")
			          Util.aguarde(200)
			          escreva("bom...\n\n")
			          Util.aguarde(2000)

					escreva("Você corre o RISCO de CONFUSÃO MENTAL...(É causada pelo frio, boa sorte).")
					Util.aguarde(4000)

				     sim()
				     pare
				     }
					
					caso 2:
					limpa()
					escreva("Você ")
					Util.aguarde(200)
					escreva("tenta ")
					Util.aguarde(200)
					escreva("enfrentar ")
					Util.aguarde(200)
					escreva("esse ")
					Util.aguarde(200)
					escreva("frio")
					Util.aguarde(200)
					escreva(".")
					Util.aguarde(200)
					escreva(".")
					Util.aguarde(200)
					escreva(".")
					Util.aguarde(3000)
					
					limpa()
					
					escreva("Mas ")
					Util.aguarde(200)
					escreva("essa ")
					Util.aguarde(200)
					escreva("foi ")
					Util.aguarde(200)
					escreva("sua ")
					Util.aguarde(200)
					escreva("pior ")
					Util.aguarde(200)
					escreva("escolha.")
					Util.aguarde(3000)
					limpa()
					escreva("Você ")
					Util.aguarde(200)
					escreva("fica ")
					Util.aguarde(200)
					escreva("com ")
					Util.aguarde(200)
					escreva("uma ")
					Util.aguarde(200)
					escreva("tremenda ")
					Util.aguarde(200)
					escreva("confusão ")
					Util.aguarde(200)
					escreva("dentro ")
					Util.aguarde(200)
					escreva("de ")
					Util.aguarde(200)
					escreva("si")
					Util.aguarde(200)
					escreva("...")
					Util.aguarde(3000)

					limpa()

					escreva("Conquista: M O R T E    C O N G E L A N T E \n")

					                    
					escreva("                     @%\n")
					Util.aguarde(30)
					escreva("                 %@@ @% @@=                 \n")
					Util.aguarde(30)
					escreva("                   #@@@@-                   \n")
					Util.aguarde(30)
					escreva("              =@@    @%   -@@  :            \n")
					Util.aguarde(30)
					escreva("       @@  .@=  =@@# @% %@@:  :@%  %@       \n")
					Util.aguarde(30)
					escreva("   %@. #@   @%     @@@@@%     *@   @% -@@   \n")
					Util.aguarde(30)
					escreva("    -@@@@:  @@       @%       @@  .@@@%.    \n")
					Util.aguarde(30)
					escreva("    =%@@@@@=#@       @%       @@*@@@@@@#.   \n")
					Util.aguarde(30)
					escreva("    *      @@@%      @%      %@@@           \n")
					Util.aguarde(30)
					escreva("       =@@@@+ %@@%   @%  .@@@+  %@@@@.      \n")
					Util.aguarde(30)
					escreva("     @@#.        +@@@@@@@@=         =%@     \n")
					Util.aguarde(30)
					escreva("                   +@@@@:                   \n")
					Util.aguarde(30)
					escreva("     %@@@+      +@@@ @%.@@@=       *@@@     \n")
					Util.aguarde(30)
					escreva("         #@@@%@@%    @%    @@@#%@@@=        \n")
					Util.aguarde(30)
					escreva("   #@@*   @@@@=      @%      -@@@%    %@@   \n")
					Util.aguarde(30)
					escreva("      @@@@+ *@       @%       @% *@@@@+     \n")
					Util.aguarde(30)
					escreva("   :@@@@@:  @@       @@       %@   @@@@%    \n")
					Util.aguarde(30)
					escreva("       %@   @+    :@@@@@%     -@-  @@  :-   \n")
					Util.aguarde(30)
					escreva("       :.  #@:  #@@- @% %@@*  .@@  -%       \n")
					Util.aguarde(30)
					escreva("              +@%    @%    @@.              \n")
