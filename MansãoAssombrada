		Util.aguarde(200)
		escreva("quer ")
		Util.aguarde(200)
		escreva("ficar? ")

		escreva("\n\nSim[1]  Não[2]\n\nSua escolha:")
			
		     inteiro medo
		     medo=2
			leia(medo)

			escolha(medo)
			{
			caso 1:
			limpa()
			escreva("Você ")
			Util.aguarde(200)
			escreva("decide ")
			Util.aguarde(200)
			escreva("ficar... ")
			Util.aguarde(200)
			escreva("você ")
			Util.aguarde(200)
			escreva("fica ")
			Util.aguarde(200)
			escreva("com ")
			Util.aguarde(200)
			escreva("confusão ")
			Util.aguarde(200)
			escreva("mental ")
			Util.aguarde(2000)
			limpa()
			escreva("Aos ")
			Util.aguarde(200)
			escreva("poucos ")
			Util.aguarde(200)
			escreva("você ")
			Util.aguarde(200)
			escreva("morre ")
			Util.aguarde(200)
			escreva("e ")
			Util.aguarde(200)
			escreva("é ")
			Util.aguarde(200)
			escreva("consumido ")
			Util.aguarde(200)
			escreva("pelas ")
			Util.aguarde(200)
			escreva("Aranhas")
			Util.aguarde(3000)

			limpa()

			escreva("CONQUISTA: M O R T E   A R A C N I D E A \n\n")

				escreva("	                                            .                                \n")
				escreva("	                                          :     +                            \n")
				escreva("	                                         -     -                             \n")
				escreva("	                                         +     =                             \n")
				escreva("	                                       .@     :+                             \n")
				escreva("	                                       #      @:                             \n")
				escreva("	                                      @-     @@                              \n")
				escreva("	                                     =+     *+                               \n")
				escreva("	                                    %:     =.:                               \n")
				escreva("	                                   #+     +-:                                \n")
				escreva("	                                  @@     +=:                                 \n")
				escreva("	                      .+@- -@:   *@*    %@.                                  \n")
				escreva("	                 .@@@@@%+===@*   -.-   %::                                   \n")
				escreva("	              +@+= -        @*-  *=.  @-:                                    \n")
				escreva("	           +@:              :%- .*+  %+-                                     \n")
				escreva("	        -=     .::           %#-+%%:@%  #                    .=*##@@@@@%=    \n")
				escreva("	              ....  ..   %@@+*@@@@%@@=%@            -##+=#=:     :. ::.      \n")
				escreva("	                    . #@@**+##@@@@*%=+@@*=    =#@@@*%-     .:.               \n")
				escreva("	                    -@+=-----*+%#@@+ =%@   :#.+@=     -=:                    \n")
				escreva("	                 = %@++=:++*==+#*@@@@=*@*=*@#    =- .                        \n")
				escreva("	             -=++@@@#==++----===+%@@@@@@+*.:.                                \n")
				escreva("	          +%@%    .@=+:--:-::--++: @@@@@@@-.:::-*@                           \n")
				escreva("	         -@-      :@=++:..:.:+#-  @@@@#*@@*:     =@#.                        \n")
				escreva("	        .#%       .@=:-.:=-+%@:  @@==@=*         :..@* :                     \n")
				escreva("	       .=@         %#-:-=*%@@:+@@-:--+@%+-         .::%%=#                   \n")
				escreva("	       -*:       . :@@@@@@@@@@@@::  .-+*@@.           .. .#*                 \n")
				escreva("	      %#%      . :::=%@*-:::@=@=..   ..=@@                . @=               \n")
				escreva("	     @@*   ...       .   . #-+# ..     :@*                   :#              \n")
				escreva("	    .@    .               :@:@: :       @+                                   \n")
				escreva("	    @-  .                 @@@. .:      -@+.                                  \n")
				escreva("	    @  .               =*@@@   :       %..                                   \n")
				escreva("	   %.            :+=#@@+.   ...      .@ .                                    \n")
				escreva("	   +.        @%+@@*-   ....         --                                       \n")
				escreva("	   -     .=#%==  .                 =                                         \n")
				escreva("	      =@=:.                       :    			 					  \n")
			}



		   
     
	}
     funcao lugart()
     {
     	     
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@%%##***++::+**********+++++++++++*+: .-========++++++*####**++++++\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@@%%#****++-.+**********++++++==+=-  :-----====++++*#*=++++++++**##\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@@%#*+++++++======-------::::...   .:::::::-------------------=====\n")     
     Util.aguarde(30)
     escreva("++*%@@@@@@@@@@@@@@@@@@@@@@@@*+=::=++++++++++++++=====--===+++.+++++++++++===.++**########*\n")     
     Util.aguarde(30)
     escreva("***#%@@@@@@@@@@@@@@@@@@@@.:*@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@\n")     
     Util.aguarde(30)
     escreva("#**#@@@@@@@@@@@@@@@@@@@@@::-+@@#=#@@%@@%#**++++*#%@@%#%@#+*@@@@@@@@@@@@@@@@@@@%*%@@@@@#@@@\n")
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@%%@@@@@@@@@.:-=@@%@@%@%%@@@%#######+#+#%%@@%*@@@@@@@*@@@@@@@@@@@==*@@@@@*@@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@%%%@.:-=@@=-*=#@@@: : ::-:.+ :@@#*#@@:@@@@@@:-+@@@@@@%%#@@@%@@@@@.   \n")     
     Util.aguarde(30)
     escreva("         ::+#@@@@@@@@%#*%--:=@@++#-*@@@+-=. .-. -.+@@#+*#- #@@@@@==%@@@@@@@@%#@+%@@@@@:=-:\n")     
     Util.aguarde(30)
     escreva("           .-@@@@@@@@@%@@@=:-@@=#*-+@@@#**+ :*+=*+@*@@%#   *@@@@@@*-+@%@@@@@@*+@%@@@@@=#*#\n")     
     Util.aguarde(30)
     escreva("           .:@@@@@@@@@@@@@=:-@@=@@@*@@@@%@@@@@*+@@@@@@%#@@-@@@@@@@@%*%@#@@@@@@=+@@@@@@*@@@\n")     
     Util.aguarde(30)
     escreva("    .      :=@@@@@@@@@@@@@=:-@@*@@@@@@@@@@@@@##=@%+@@%@@@@*@@@@@@@@@@@%%@@@@@@@@@@@@@@#@@@\n")     
     Util.aguarde(30)
     escreva(".   .:-:*%@@@@@@@@@@@@@@@@@@@@@#@@@#=+-+-+-+-;+-=-=-+_+_-;+-=-=-+_+_-;+-=-=-+_+_-;+-=-=-+_\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@@@@@@#=+*%###%@%%#%@@#*#@*%@@@@@@@@@#@@@@@%**#+==---=+**++=@@@@@@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@@@%#@+*@@@@@@@++.-@@@@@@#**##+%@@#*##@@@@@@@@@@@#%@@@%*=:::+@@@@@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@%=:::---@@@@@==-:-@@@@+:--=+**#@@@@@@@@@@@@@@@%%@@@=-\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@@@@%*+#@@@@@@@@@%@@@@@@@@@@@@@@%===*@@@@#===+**#%@@@@@@@@@@@.--+@**@@\n")    
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@@@@%#*++--@@@@@@@@#+@@@@@@@@@@@@@@@-:--=%@@@@@%*+**##%@@@@@@@*-.-+**##@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@@@@%#*++=--+@@@@@@@@--+@@@@@@@==-:=-=-..--:-=@@@@@@@#**#%%%%%%%@%#*++-:=#@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@@@@##*++==--=**+++++++===--------.......   .+===---------=====#@#%+%%%%@%##%@*\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@@@@@#*++==-:--=+++=+==++=-===-------=........    -=-----:::::---=*@#@@@@@@@@@@@@@@\n")     
     Util.aguarde(30)
     escreva("@@@@@@@@#**+=----::--++++++=+==----*========+........     -------:::::::=%#@@@@@@@@@@@@@@@\n")     
     Util.aguarde(30)
     escreva("@@@@@#*#**+==----==**+++++==------+*++++++++*-:::..:.:..  .=+===-=------===%@@@@@@@@@@@@@@\n")
	Util.aguarde(30)
	escreva("                             || Lugar ")
	Util.aguarde(30)
	escreva("desconhecido ||\n")
	Util.aguarde(2000)
     escreva("                             ||   Ameaça:4 de 10   ||")
     Util.aguarde(800)
     escreva("\nAMEAÇAS: comidas venenosas, confusão mental, escorpiôes e 'presença estranha'")
     escreva("(como se algo te dissese para sair o mais rapido possivel).\n\n")

	inteiro sair
		
		escreva("\nAVISO, LEIA AS INFORMAÇÔES, ESCREVA 1 PARA SAIR:")
		leia(sair)
		se(sair == 1)
		{
			DA()
		}
     
     }

	
	funcao imagemDado()
	{
		
				
		escreva("\n\n	      -**_**-*-*--**-*%%%%*-                     \n")
          escreva("           +%##. .=+*#-   -#=  =##%%%=              \n")
          escreva("       =%%%%%%##*:.:*#.  :     -**+:  =%.           \n")
          escreva("       #%@%+ *%%%%%%####:  +##=:.+*#*=*%%#          \n")
          escreva("       #%%@@@@%%%%%%%%#*- +*#####+-%%%%#*+          \n")
          escreva("      *###%%%%@@@@@%%@@%%#-..:%@%%%##***+           \n")
          escreva("      *##%##%##%%@@@@@@@@@@@@%%####*****+           \n")
          escreva("      -%%#.    .#%%%%@@@@@%%#####*    +*+           \n")
          escreva("      .%%=   #%  :#%%%%@@@@%%##+  +-  +++           \n")
          escreva("       #%-  -##.  ##%%%%@%%%%#-  =*..+**=           \n")
          escreva("       #%+      =##%%%%%%%%%##   =+****+            \n")
          escreva("       #%#   #%   #%%%%%%%%###    +  +*+            \n")
          escreva("       :#%#  ##*   %%%%%%%%###*  *+ .**-            \n")
          escreva("        :##%= +-   #%%%%%%%###   . =**.             \n")
          escreva("           #%%#: .+%%%%%%%%###. .+##=               \n")
          escreva("              *%@%%%%%%%%%%%#####*                  \n")
          escreva("                 =@@%%%%%%%%##*                     \n")
          escreva("                     @%%%%%%                        \n")

          



      }



      
}
