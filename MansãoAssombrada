escreva("\n	    	      ##              \n")
escreva("	         @@# @@@@.*@@.        \n")
escreva("	    +*-.:@@@@@@@@@@@@- -**    \n")
escreva("	    .@@@@@@@*.  .+@@@@@@@:    \n")
escreva("	    :%@@@: -@@@@@@- .@@@%:.   \n")
escreva("	 .@@@@@* #@@@@@@@@@@# +@@@@@. \n")
escreva("	   %@@% @@@@@@@@@@@@@@ #@@%   \n")
escreva("	 *@@@@:-@@@@@@@@@@@@@@=.@@@@# \n")
escreva("	 +@@@@:-@@@@@@@@@@@@@@-.@@@@* \n")
escreva("	   @@@% %@@@@@@@@@@@@@ #@@@   \n")
escreva("	 .@@@@@* *@@@@@@@@@@# +@@@@@. \n")
escreva("	     %@@@- :@@@@@@: :@@@%.    \n")
escreva("	    :@@@@@@@#:  :#@@@@@@@:    \n")
escreva("	    ++. @@@@@@@@@@@@@@ .+*    \n")
escreva("	       =@@@@@@@@@@@@@@*       \n")
escreva("	       @@@@@@@@@@@@@@@@.      \n")
escreva("	      %@@@@@@@%#@@@@@@@@      \n")
escreva("	     -@@@@@@@@:.@@@@@@@@=     \n")
escreva("	     @@@@%@@@+  +@@@@@@@@.    \n")
escreva("	    #@=  :@@#    #@@:  =@#    \n")
escreva("	   ..     *@:    :@*      .   \n")
escreva("         .-      -:      .-       ,  \n")



