	funcao sim()
	{
					escreva("Você ")
					Util.aguarde(200)
					escreva("vê ")
					Util.aguarde(200)
					escreva("uma ")
					Util.aguarde(200)
					escreva("porta ")
					Util.aguarde(200)
					escreva("dentro ")
					Util.aguarde(200)
					escreva("do ")
					Util.aguarde(200)
					escreva("guarda ")
					Util.aguarde(200)
					escreva("roupa, ")
					Util.aguarde(3000)
					escreva("você ")
					Util.aguarde(200)
					escreva("suspeita ")
					Util.aguarde(200)
					escreva("disso")
					Util.aguarde(200)
					escreva(".")
					Util.aguarde(200)
					escreva(".")
					Util.aguarde(200)
					escreva(".")
					Util.aguarde(3000)

					limpa()
					
					escreva("Ariscar? ")
					Util.aguarde(800)
				     
				     escreva("\n\nSim[1]  Não[2]\n\nSua escolha:")
					
				     inteiro a=2
					leia(a)

					escolha(a)
					{

					caso 1:

					pool()

		pare
   		
		caso 2:
			
	     des()

	     pare


     }
			
				
	}
	funcao des()
     {
     	limpa()
     	escreva("Você ")
		Util.aguarde(200)
		escreva("percebe ")
		Util.aguarde(200)
		escreva("que ")
		Util.aguarde(200)
		escreva("não ")
		Util.aguarde(200)
		escreva("há ")
		Util.aguarde(200)
		escreva("nada ")
		Util.aguarde(200)
		escreva("de ")
		Util.aguarde(200)
		escreva("interresante ")
		Util.aguarde(200)
		escreva("na ")
		Util.aguarde(200)
		escreva("casa...")
		Util.aguarde(2000)
		limpa()
		escreva("Você ")
		Util.aguarde(200)
		escreva("ainda ")
		Util.aguarde(200)
		escreva("quer ")
		Util.aguarde(200)
		escreva("ficar? ")

		escreva("\n\nSim[1]  Não[2]\n\nSua escolha:")
			
		     inteiro medo
		     medo=2
			leia(medo)

			escolha(medo)
			{
			caso 1:
			limpa()
			escreva("Você ")
			Util.aguarde(200)
			escreva("decide ")
			Util.aguarde(200)
			escreva("ficar... ")
			Util.aguarde(200)
			escreva("você ")
			Util.aguarde(200)
			escreva("fica ")
			Util.aguarde(200)
			escreva("com ")
			Util.aguarde(200)
			escreva("confusão ")
			Util.aguarde(200)
			escreva("mental ")
			Util.aguarde(2000)
			limpa()
			escreva("Aos ")
			Util.aguarde(200)
			escreva("poucos ")
			Util.aguarde(200)
			escreva("você ")
			Util.aguarde(200)
			escreva("morre ")
			Util.aguarde(200)
			escreva("e ")
			Util.aguarde(200)
			escreva("é ")
			Util.aguarde(200)
			escreva("consumido ")
			Util.aguarde(200)
			escreva("pelas ")
			Util.aguarde(200)
			escreva("Aranhas")
			Util.aguarde(3000)

			limpa()

			escreva("CONQUISTA: M O R T E   A R A C N I D E A \n\n")

				escreva("	                                            .                                \n")
				escreva("	                                          :     +                            \n")
				escreva("	                                         -     -                             \n")
				escreva("	                                         +     =                             \n")
				escreva("	                                       .@     :+                             \n")
				escreva("	                                       #      @:                             \n")
				escreva("	                                      @-     @@                              \n")
				escreva("	                                     =+     *+                               \n")
				escreva("	                                    %:     =.:                               \n")
				escreva("	                                   #+     +-:                                \n")
				escreva("	                                  @@     +=:                                 \n")
				escreva("	                      .+@- -@:   *@*    %@.                                  \n")
				escreva("	                 .@@@@@%+===@*   -.-   %::                                   \n")
				escreva("	              +@+= -        @*-  *=.  @-:                                    \n")
				escreva("	           +@:              :%- .*+  %+-                                     \n")
				escreva("	        -=     .::           %#-+%%:@%  #                    .=*##@@@@@%=    \n")
				escreva("	              ....  ..   %@@+*@@@@%@@=%@            -##+=#=:     :. ::.      \n")
				escreva("	                    . #@@**+##@@@@*%=+@@*=    =#@@@*%-     .:.               \n")
				escreva("	                    -@+=-----*+%#@@+ =%@   :#.+@=     -=:                    \n")
				escreva("	                 = %@++=:++*==+#*@@@@=*@*=*@#    =- .                        \n")
				escreva("	             -=++@@@#==++----===+%@@@@@@+*.:.                                \n")
				escreva("	          +%@%    .@=+:--:-::--++: @@@@@@@-.:::-*@                           \n")
				escreva("	         -@-      :@=++:..:.:+#-  @@@@#*@@*:     =@#.                        \n")
				escreva("	        .#%       .@=:-.:=-+%@:  @@==@=*         :..@* :                     \n")
				escreva("	       .=@         %#-:-=*%@@:+@@-:--+@%+-         .::%%=#                   \n")
				escreva("	       -*:       . :@@@@@@@@@@@@::  .-+*@@.           .. .#*                 \n")
				escreva("	      %#%      . :::=%@*-:::@=@=..   ..=@@                . @=               \n")
				escreva("	     @@*   ...       .   . #-+# ..     :@*                   :#              \n")
				escreva("	    .@    .               :@:@: :       @+                                   \n")
				escreva("	    @-  .                 @@@. .:      -@+.                                  \n")
				escreva("	    @  .               =*@@@   :       %..                                   \n")
				escreva("	   %.            :+=#@@+.   ...      .@ .                                    \n")
				escreva("	   +.        @%+@@*-   ....         --                                       \n")
				escreva("	   -     .=#%==  .                 =                                         \n")
				escreva("	      =@=:.                       :    			 					  \n")
			
				caso 2:

				pool()
			}


				
		   
     }
	funcao pool()
	{
			limpa()
					escreva("Você ")
					Util.aguarde(200)
					escreva("entra")
					Util.aguarde(200)
					escreva(".")
					Util.aguarde(500)
					escreva(".")
					Util.aguarde(500)
					escreva(".")
					Util.aguarde(3000)
					escreva("Que ")
					Util.aguarde(200)
					escreva("lugar ")
					Util.aguarde(200)
					escreva("é ")
					Util.aguarde(200)
					escreva("esse?? ")
					Util.aguarde(3000)
					limpa()

    escreva("=======================+++++++++++++++++++++\n")
    escreva("-----=================+++++++++++++====+++++    \n")
    escreva("------==============+++++++++++++++======--:    \n")
    escreva("--------==========+++++++++++++++++==-:.        \n")
    escreva("---------=========+++++++++++++++++=:..         \n")
    escreva("----------=======++++++++++++++++*+=:...        \n")
    escreva("==============+++++++++************=:..         \n")
    escreva("+++++++++**#*****************#*****=:...        \n")
    escreva("=======+++*%%%%%%#################*=:...        \n")
    escreva("=========+*%@%%%%%%%%%%%%%%%%%%###*=:...        \n")
    escreva("          :%@@@%%@@@@@@@@@@##%###*+=:..         \n")
    escreva("          .#@@####%@@@@@@@%#####*++=:....       \n")
    escreva("          .#@%---=@@@@@@@@%####*+:.-:...        \n")
    escreva("          .#%#:::=%@@@@@@@%###**+..::..         \n")
    escreva("          .#%* ..#@@@@@@@@%**#*+=  ::...        \n")
    escreva("           %@#--+*@@@@@@@@%#*##*+..::...        \n")
    escreva("          .%@%***#%@@@@@@@%%%%%%#===:..         \n")
    escreva("         .:###****######%%%%%%%%#*+=:..         \n")
    escreva(".........::**************####%%%###=:.          \n")
    escreva(".......:::-++++++++********########=:.          \n")
    escreva("::::::::::-++++++++++++******######=.           \n")
    escreva("::::::::---++++++++++++++++++**####*=:.         \n")
    escreva("::--:::---=++++++++++++++++++++*###*+==----:    \n")
    escreva("----------=+++++++++++++++++++++**#*+=====--    \n")
    escreva("--------=+++++++++++++++++++++++++**+=----::    \n")
    escreva("+++=+++++++++++++++++++++++++++++++++=----::    \n")
    escreva("+++++++++++++++++++++++++++++++++++++=----::    \n")

    escreva("        ||  'Shallow' ")
	Util.aguarde(30)
	escreva("Waters   ||\n")
	Util.aguarde(2000)
     escreva("        ||   Ameaça:5 de 10   ||")
     Util.aguarde(800)
     escreva("\nAMEAÇAS: piranhas, em alguns lugares águas acidas, portas devoradoras, ansiedade,  e 'presença estranha'(Ele quer te AFOGAR)")
     escreva("\nPONTOS POSITIVO: comidas saudaveis\n\n")

	inteiro sair
		
		escreva("\nAVISO, LEIA AS INFORMAÇÔES, ESCREVA 1 PARA SAIR:")
		leia(sair)
		se(sair == 1)
		{
			Aguas()
		}

	}
