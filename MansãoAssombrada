	funcao sim()
	{
					escreva("Você ")
					Util.aguarde(200)
					escreva("vê ")
					Util.aguarde(200)
					escreva("uma ")
					Util.aguarde(200)
					escreva("porta ")
					Util.aguarde(200)
					escreva("dentro ")
					Util.aguarde(200)
					escreva("do ")
					Util.aguarde(200)
					escreva("guarda ")
					Util.aguarde(200)
					escreva("roupa, ")
					Util.aguarde(3000)
					escreva("você ")
					Util.aguarde(200)
					escreva("suspeita ")
					Util.aguarde(200)
					escreva("disso")
					Util.aguarde(200)
					escreva(".")
					Util.aguarde(200)
					escreva(".")
					Util.aguarde(200)
					escreva(".")
					Util.aguarde(3000)

					limpa()
					
					escreva("Ariscar? ")
					Util.aguarde(800)
				     
				     escreva("\n\nSim[1]  Passar frio[2]\n\nSua escolha:")
					
				     inteiro a=2
					leia(a)

					escolha(a)
					{

					caso 1:
					limpa()
					escreva("Você ")
					Util.aguarde(200)
					escreva("entra")
					Util.aguarde(200)
					escreva(".")
					Util.aguarde(500)
					escreva(".")
					Util.aguarde(500)
					escreva(".")
					Util.aguarde(3000)
					escreva("Que ")
					Util.aguarde(200)
					escreva("lugar ")
					Util.aguarde(200)
					escreva("é ")
					Util.aguarde(200)
					escreva("esse?? ")
					Util.aguarde(3000)
					limpa()

    escreva("=======================+++++++++++++++++++++\n")
    escreva("-----=================+++++++++++++====+++++    \n")
    escreva("------==============+++++++++++++++======--:    \n")
    escreva("--------==========+++++++++++++++++==-:.        \n")
    escreva("---------=========+++++++++++++++++=:..         \n")
    escreva("----------=======++++++++++++++++*+=:...        \n")
    escreva("==============+++++++++************=:..         \n")
    escreva("+++++++++**#*****************#*****=:...        \n")
    escreva("=======+++*%%%%%%#################*=:...        \n")
    escreva("=========+*%@%%%%%%%%%%%%%%%%%%###*=:...        \n")
    escreva("          :%@@@%%@@@@@@@@@@##%###*+=:..         \n")
    escreva("          .#@@####%@@@@@@@%#####*++=:....       \n")
    escreva("          .#@%---=@@@@@@@@%####*+:.-:...        \n")
    escreva("          .#%#:::=%@@@@@@@%###**+..::..         \n")
    escreva("          .#%* ..#@@@@@@@@%**#*+=  ::...        \n")
    escreva("           %@#--+*@@@@@@@@%#*##*+..::...        \n")
    escreva("          .%@%***#%@@@@@@@%%%%%%#===:..         \n")
    escreva("         .:###****######%%%%%%%%#*+=:..         \n")
    escreva(".........::**************####%%%###=:.          \n")
    escreva(".......:::-++++++++********########=:.          \n")
    escreva("::::::::::-++++++++++++******######=.           \n")
    escreva("::::::::---++++++++++++++++++**####*=:.         \n")
    escreva("::--:::---=++++++++++++++++++++*###*+==----:    \n")
    escreva("----------=+++++++++++++++++++++**#*+=====--    \n")
    escreva("--------=+++++++++++++++++++++++++**+=----::    \n")
    escreva("+++=+++++++++++++++++++++++++++++++++=----::    \n")
    escreva("+++++++++++++++++++++++++++++++++++++=----::    \n")

    escreva("        || 'Shallow' ")
	Util.aguarde(30)
	escreva("Waters   ||\n")
	Util.aguarde(2000)
     escreva("        ||   Ameaça:5 de 10   ||")
     Util.aguarde(800)
     escreva("\nAMEAÇAS: lugares fundos, piranhas, suco venenoso, ansiedade,  e 'presença estranha'")
     escreva("\nPONTOS POSITIVO: comidas saudaveis\n\n")

	inteiro sair
		
		escreva("\nAVISO, LEIA AS INFORMAÇÔES, ESCREVA 1 PARA SAIR:")
		leia(sair)
		se(sair == 1)
		{

			
		}

		pare
   		
		caso 2:
			
	     des()

	     pare


     }
			
				
	}
