		escreva("\n============================----=================----===========---------=======\n")
          escreva("=====-.========.-=======:.========.-=======-..=======.-=======-.========.-======\n")
 		escreva("=====:__________-=======:_________--=======+ _________-=======* __________-=====\n")
 		escreva("====+|  ....... |=======|  ......:|-=======+| ........|=======*| .........|=====\n")
 		escreva("====+|    @  .. |=======|  . %  .:|-=======+| .  # ...|=======*|     $ ...|=====\n")
 		escreva("====+|       .. |======+|       .:|-=======+|       ..|=======*|       ...|=====\n")
 		escreva("====+| *      . |======+| :      :|-=======+|-.    ...|=======*|+      ...|=====\n")
 		escreva("====+| ........ |======+| ...... :|-=======+|.........|=======*|...... ...|=====\n")
 		escreva("====+|  ....... |======+|  ......:|-=======+| ........|=======*| .........|=====\n")
		escreva("====+|        . |=======|        :|-=======+| .    ...|=======*|       ...|=====\n")
		escreva("-----|.       ..|-------|:  ..   .| --------|  . .  ..|:-:-----|  .     ..|:::::\n")
		escreva("________________________________________________________________________________\n")
		escreva("________________________________________________________________________________\n")
		escreva("________________________________________________________________________________\n")
		escreva("________________________________________________________________________________\n")
		escreva("________________________________________________________________________________\n")
		escreva("________________________________________________________________________________\n")
		
		escreva("\nVocê ")
     	Util.aguarde(600)
     	
		escreva("nota ")
     	Util.aguarde(200)
     	
     	escreva("que ")
     	Util.aguarde(200)
     	
     	escreva("apenas ")
     	Util.aguarde(200)
     	
    		escreva("a ")
     	Util.aguarde(200)	
		
    		escreva("primeira ")
     	Util.aguarde(200)
     	
    		escreva("porta ")
     	Util.aguarde(200)	
    		
    		escreva("esta ")
     	Util.aguarde(200)

    		escreva("aberta.")
     	Util.aguarde(2500)

     	 decisaoAoDesafio()


	}
	funcao decisaoAoDesafio()
	{
		
		inteiro vPOR
		
		escreva("Você")
		Util.aguarde(200)
		
		escreva(" quer ")
		Util.aguarde(200)
		
		escreva("entrar? ")
		Util.aguarde(200)
	
		escreva("\n\nSim[1]  Não[2]\n\nSua escolha:")
		
    		leia(vPOR)

		    		
			escolha(vPOR)
			{
			caso 1:

				limpa()
				mansaoEstaDiferente()
				pare
				
		    	caso 2:

					limpa()
			
     				
    					escreva("você ")
     				Util.aguarde(200)

    					escreva("tenta ")
     				Util.aguarde(200)
     				
    					escreva("abrir ")
     				Util.aguarde(200)
     				
    					escreva("a ")
     				Util.aguarde(200)
     				
    					escreva("porta ")
     				Util.aguarde(200)
     				
    					escreva("de ")
     				Util.aguarde(200)
     				
    					escreva("onde ")
     				Util.aguarde(200)
     				
    					escreva("você ")
     				Util.aguarde(200)
     				
    					escreva("veio, ")
     				Util.aguarde(1000)
     				
    					escreva("ela ")
     				Util.aguarde(200)
     				
    					escreva("está ")
     				Util.aguarde(200)
     				
    					escreva("trancada, ")
     				Util.aguarde(1000)
     				
    					escreva("você ")
     				Util.aguarde(200)
     				
    					escreva("está ")
     				Util.aguarde(200)
     				
    					escreva("preso.")
     				Util.aguarde(2000)

					limpa()
					
					semVolta()

					pare
     				
		    	}	


			
	}
	funcao semVolta()
	{
		
		inteiro vPOR=1
		
		escreva("Entrar?? ")
		Util.aguarde(200)

		
		escreva("\n\nSim[1]\n\nSua escolha:")
		
    		leia(vPOR)

    		se(vPOR == 1)
    		{

			mansaoEstaDiferente()
    			
    		}
		
	}
	funcao mansaoEstaDiferente()
	{

		limpa()
			escreva("Você ")
			Util.aguarde(200)
			escreva("entra ")
			Util.aguarde(200)
			escreva("na ")
			Util.aguarde(200)
			escreva("porta")
			Util.aguarde(200)
			
			Util.aguarde(800)
			escreva(".")
			Util.aguarde(800)
			escreva(".")
			Util.aguarde(800)
			escreva(".")
		
			escreva(" e ")
			Util.aguarde(200)
			escreva("percebe ")
			Util.aguarde(200)
			escreva("que ")
			Util.aguarde(200)
			escreva("as ")
			Util.aguarde(200)
			escreva("paredes ")
			Util.aguarde(200)
			escreva("de ")
			Util.aguarde(200)
			escreva("madeira ")
			Util.aguarde(200)
			escreva("não ")
			Util.aguarde(200)
			escreva("estão ")
			Util.aguarde(200)
			escreva("mais ")
			Util.aguarde(200)
			escreva("lá.")
			Util.aguarde(2000)

			limpa()
			
			escreva("Mas ")
			Util.aguarde(200)
			escreva("paredes ")
			Util.aguarde(200)
			escreva("brancas, ")
			Util.aguarde(800)
			escreva("'parece' ")
			Util.aguarde(200)
			escreva("um ")
			Util.aguarde(200)
			escreva("belo ")
			Util.aguarde(200)
			escreva("lugar.")
			Util.aguarde(2000)

			limpa()
			
			lugart()
	}
	funcao DA()
	{
			limpa()
			escreva("Você ")
			Util.aguarde(200)
			escreva("vê ")
			Util.aguarde(800)
			escreva("um ")
			Util.aguarde(200)
			escreva("belo ")
			Util.aguarde(200)
			escreva("banquete, ")
			Util.aguarde(200)

			escreva("comer?? ")
			Util.aguarde(200)
		     
		     escreva("\n\nSim[1]  Não[2]\n\nSua escolha:")

			inteiro decida
		     
		     decida=2
		     leia(decida)
		     escolha(decida)
		     {
		     	caso 1:
					limpa()
		     	escreva("CONQUISTA: F I N A L    V E N E N O S O ")
		     	escreva("\n\n")
				escreva("                              *@@@#                        \n")                         
                    escreva("                         :@@@@@@@@@@@@@@@:                 \n")                          
                    escreva("    tenha              @@@@@@@@@@@@@@@@@@@@@               \n")                          
                    escreva("      mais           =@@@@@@@@@@@@@@@@@@@@@@@+             \n")                          
                    escreva("         atenção     @@@@@@@@@@@@@@@@@@@@@@@@@             \n")                          
                    escreva("                    +@@@@@@@@@@@@@@@@@@@@@@@@@+            \n")                          
                    escreva("     na              @@@@@@@@@@@@@@@@@@@@@@@@@.            \n")                          
                    escreva("       próxima       @@@@@     @@@@@     @@@@@             \n")                          
                    escreva("             vez!    @@@       @@@       #@@              \n")                          
                    escreva("                       @@     @@@@@@@     @@               \n")                          
                    escreva("                      =@@@@@@@@@   @@@@@@@@@=              \n")                          
                    escreva("                          -@@@@@@@@@@@@@-                  \n")                          
                    escreva("               @@@@         @@@@@@@@@@@         @@@@         \n")                          
                    escreva("              =@@@@@        =@@@@@@@@@=        @@@@@=        \n")                          
                    escreva("                :@@@@@@@@@=             -@@@@@@@@@-      \n")                          
                    escreva("                 @@@@@@@@@@@@@@     @@@@@@@@@@@@@@       \n")                          
                    escreva("                             @@@@@@@@@                   \n")                          
                    escreva("                          @@@@@@@@@@@@@@@                \n")                          
                    escreva("                @@@@@@@@@@@@*         +@@@@@@@@@@@@      \n")                          
                    escreva("                 =@@@@@@@                 @@@@@@@*       \n")                          
                    escreva("                  %@@@@                     @@@@%        \n")                          
                    escreva("                    @=                       -@      	  \n")
		     	pare
		     	
		     	caso 2:

				limpa()
				
		          escreva("Você ")
				Util.aguarde(30)
				escreva("resiste ")
				Util.aguarde(30)
				escreva("e ")
				Util.aguarde(30)
				escreva("não ")
				Util.aguarde(30)
				escreva("come, ")
				Util.aguarde(30)
				escreva("sua ")
				Util.aguarde(30)
				escreva("barriga ")
				Util.aguarde(30)
				escreva("doe ")
				Util.aguarde(30)
				escreva("de ")
				Util.aguarde(30)
				escreva("fome.")
				Util.aguarde(2800)

				dorp()
				
		     }
	}
	funcao dorp()
	{


			limpa()
		     
     		escreva("Você ")
     		Util.aguarde(200)
     		escreva("sente ")
     		Util.aguarde(200)
     		escreva("frio ")
     		Util.aguarde(200)
     		escreva("e ")
     		Util.aguarde(200)
     		escreva("tem ")
     		Util.aguarde(200)
     		escreva("a ")
     		Util.aguarde(200)
     		escreva("ideia ")
     		Util.aguarde(200)
     		escreva("de ")
     		Util.aguarde(200)
     		escreva("ir ")
     		Util.aguarde(200)
     		escreva("no ")
     		Util.aguarde(200)
     		escreva("quarto ")
     		Util.aguarde(200)
     		escreva("para ")
     		Util.aguarde(200)
     		escreva("pegar ")
     		Util.aguarde(200)
     		escreva("um ")
     		Util.aguarde(200)
     		escreva("casaco")
     		Util.aguarde(3600)

     		limpa()
     		
			escreva("Pegar ")
			Util.aguarde(200)
			escreva("casaco? ")
			Util.aguarde(800)
		     
		     escreva("\n\nSim[1]  Passar frio[2]\n\nSua escolha:")
			
		     inteiro medo
		     medo=2
			leia(medo)
		     escolha(medo)
		     {
		     	caso 1:
				limpa()
		     	escreva("O ")
		     	Util.aguarde(200)
		     	escreva("casaco ")
		     	Util.aguarde(200)
		     	escreva("esta ")
